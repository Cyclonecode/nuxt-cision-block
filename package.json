{
  "name": "nuxt-cision-block",
  "version": "1.0.0",
  "description": "Cision Block module for nuxt",
  "engines": {
    "node": ">=16.13"
  },
  "main": "lib/index",
  "files": [
    "lib",
    "README.md"
  ],
  "typings": [
    "lib/index"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "postbuild": "cp -R src/*.vue src/pages src/*.js lib/.",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "Krister Andersson <cyclonecode@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyclonecode/nuxt-cision-block.git"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "lru-cache": "^6.0.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.0",
    "@types/lru-cache": "^5.1.1",
    "@types/md5": "^2.3.1",
    "@types/node": "^17.0.8",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4",
    "vue": "^2.6.14"
  }
}
